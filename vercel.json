{
  "version": 2,
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/_atom/ws",
      "dest": "/api/atom-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/atom-server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}