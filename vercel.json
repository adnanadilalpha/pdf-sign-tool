{
  "version": 2,
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/_atom/ws",
      "dest": "/api/atom-server.js"
    },
    {
      "src": "/_atom/image",
      "dest": "/api/atom-server.js"
    },
    {
      "src": "/_atom/revalidate",
      "dest": "/api/atom-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/atom-server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/atom-server.js": {
      "maxDuration": 10,
      "includeFiles": "public/**/*"
    }
  }
}